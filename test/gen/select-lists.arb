--! arbre run $FILE

main =
    a := [1, 0]
    g := a              ? | [x, y]    : y                     | x         : 1
    h := [1, 2, 3]      ? | []        : 1                     | [1]       : 1   | [1, 2, 3] : 0
    i := [0]            ? | []        : 1                     | [0, 0, 0] : 1   | [0]       : 0
    j := []             ? | [0]       : 1                     | [0, 0, 0] : 1   | []        : 0
    k := [0, 1, 2]      ? | [x, xs..] : 0                     | [1, 2, 3] : 1
    l := [0]            ? | [x]       : x                     | [0]       : 1
    m := [1, 1]         ? | [x, y]    : x - y                 | [1, 1]    : 1
    n := [1, 1]         ? | [x, x]    : x - x                 | [1, 1]    : 1
    o := [1, 2]         ? | [x, x]    : 1                     | [x, 2]    : 0
    p := ['a, 'b]       ? | [x, x]    : 1                     | [x, y]    : 0
    q := [1, 1, 0]      ? | [x, xs..] : xs ? [x, y] : y
    g + h + i + j + k + l + m + n + o + p + q
